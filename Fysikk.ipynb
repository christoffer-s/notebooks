  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Tid = 3.0 sekunder \n",
      "Strekning = 15.0 meter \n",
      "Hastighet = 5 meter per sekund\n"
     ]
    }
   ],
   "source": [
    "# Simulering og beregning av hastighet og akselerasjon\n",
    "\n",
    "v = 5\n",
    "s = 0\n",
    "t = 0\n",
    "dt = 0.1\n",
    "ts = 3\n",
    "\n",
    "while t < ts:\n",
    "    s += v*dt\n",
    "    t += dt\n",
    "print(f'Tid = {round(t, 5)} sekunder \\nStrekning = {s} meter \\nHastighet = {v} meter per sekund')\n"
   ]
  }
